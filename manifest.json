{
  "name": "APP_NAME",
  "id": "APP_NAME",
  "description": {
    "en": "Access to APP_NAME",
    "fr": "Accès à un APP_NAME"
  },
  "developer": {
    "name": "Clément scrouillou Vannier",
    "email": "clement@vannier.pro",
    "url": "http://yunohost.org"
  },
  "multi_instance": "false",
  "arguments": {
    "install": [
      {
        "name": "domain",
        "ask": {
          "en": "Choose a domain for APP_NAME"
        },
        "example": "domain.org"
      },
      {
        "name": "path",
        "ask": {
          "en": "Choose a path for APP_NAME"
        },
        "example": "/",
        "default": "/"
      },
      {
        "name": "server",
        "ask": {
          "en": "Choose IP where APP_NAME is installed"
        },
        "example": "127.0.0.1",
        "default": "127.0.0.1"
      }
    ]
  }
}
